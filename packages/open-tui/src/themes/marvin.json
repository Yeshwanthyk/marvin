{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "darkBg": "#08090e",
    "darkBgPanel": "#0c0e14",
    "darkBgElement": "#12141c",
    "darkBgMenu": "#0c0e14",
    "darkBorder": "#2a3040",
    "darkBorderSubtle": "#1a1e28",
    "darkFg": "#d4d8e4",
    "darkFgMuted": "#9098a8",
    "darkComment": "#6a7080",
    "darkKeyword": "#8898b8",
    "darkCyan": "#7cb8dc",
    "darkString": "#d0a8c0",
    "darkMethod": "#e8c080",
    "darkNumber": "#e0a070",
    "darkRed": "#e07878",
    "darkYellow": "#dcb060",
    "darkPurple": "#b898c8",
    "darkVisual": "#1a1e2a",

    "lightBg": "#fffaf2",
    "lightBgPanel": "#fff6e8",
    "lightBgElement": "#f8ead7",
    "lightBgMenu": "#fff6e8",
    "lightBorder": "#e3c3a1",
    "lightBorderSubtle": "#f8ead7",
    "lightFg": "#22160f",
    "lightFgMuted": "#9d816d",
    "lightComment": "#b09382",
    "lightKeyword": "#22160f",
    "lightCyan": "#2ca7a2",
    "lightString": "#ff5c8f",
    "lightMethod": "#ff9f1c",
    "lightNumber": "#8b6dff",
    "lightRed": "#e04060",
    "lightYellow": "#ff9f1c",
    "lightPurple": "#8b6dff",
    "lightVisual": "#ffd9c6",
    "lightVariable": "#6b5548"
  },
  "theme": {
    "primary": { "dark": "darkMethod", "light": "lightMethod" },
    "secondary": { "dark": "darkCyan", "light": "lightCyan" },
    "accent": { "dark": "darkCyan", "light": "lightCyan" },
    "error": { "dark": "darkRed", "light": "lightRed" },
    "warning": { "dark": "darkYellow", "light": "lightYellow" },
    "success": { "dark": "darkCyan", "light": "lightCyan" },
    "info": { "dark": "darkCyan", "light": "lightCyan" },

    "text": { "dark": "darkFg", "light": "lightFg" },
    "textMuted": { "dark": "darkFgMuted", "light": "lightFgMuted" },

    "background": { "dark": "darkBg", "light": "lightBg" },
    "backgroundPanel": { "dark": "darkBgPanel", "light": "lightBgPanel" },
    "backgroundElement": { "dark": "darkBgElement", "light": "lightBgElement" },
    "backgroundMenu": { "dark": "darkBgMenu", "light": "lightBgMenu" },

    "border": { "dark": "darkBorder", "light": "lightBorder" },
    "borderSubtle": { "dark": "darkBorderSubtle", "light": "lightBorderSubtle" },
    "borderActive": { "dark": "darkCyan", "light": "lightCyan" },

    "selectionBg": { "dark": "darkVisual", "light": "lightVisual" },
    "selectionFg": { "dark": "darkFg", "light": "lightFg" },

    "diffAdded": { "dark": "darkCyan", "light": "lightCyan" },
    "diffRemoved": { "dark": "darkRed", "light": "lightRed" },
    "diffContext": { "dark": "darkFgMuted", "light": "lightFgMuted" },
    "diffAddedBg": { "dark": "#0c1a1a", "light": "#d4f4f0" },
    "diffRemovedBg": { "dark": "#1a0c0c", "light": "#fdd" },
    "diffAddedSign": { "dark": "darkCyan", "light": "lightCyan" },
    "diffRemovedSign": { "dark": "darkRed", "light": "lightRed" },
    "diffHighlightAddedBg": { "dark": "#143030", "light": "#b0e8e0" },
    "diffHighlightRemovedBg": { "dark": "#301414", "light": "#fcc" },

    "markdownText": { "dark": "darkFg", "light": "lightFg" },
    "markdownHeading": { "dark": "darkMethod", "light": "lightMethod" },
    "markdownLink": { "dark": "darkCyan", "light": "lightCyan" },
    "markdownLinkUrl": { "dark": "darkFgMuted", "light": "lightFgMuted" },
    "markdownCode": { "dark": "darkString", "light": "lightString" },
    "markdownCodeBlock": { "dark": "darkFg", "light": "lightFg" },
    "markdownCodeBlockBorder": { "dark": "darkBorder", "light": "lightBorder" },
    "markdownBlockQuote": { "dark": "darkFgMuted", "light": "lightFgMuted" },
    "markdownBlockQuoteBorder": { "dark": "darkBorder", "light": "lightBorder" },
    "markdownHorizontalRule": { "dark": "darkBorder", "light": "lightBorder" },
    "markdownListBullet": { "dark": "darkMethod", "light": "lightMethod" },
    "markdownStrong": { "dark": "darkFg", "light": "lightFg" },
    "markdownEmph": { "dark": "darkString", "light": "lightString" },

    "syntaxComment": { "dark": "darkComment", "light": "lightComment" },
    "syntaxString": { "dark": "darkString", "light": "lightString" },
    "syntaxKeyword": { "dark": "darkKeyword", "light": "lightKeyword" },
    "syntaxFunction": { "dark": "darkMethod", "light": "lightMethod" },
    "syntaxVariable": { "dark": "darkFg", "light": "lightVariable" },
    "syntaxType": { "dark": "darkCyan", "light": "lightCyan" },
    "syntaxNumber": { "dark": "darkNumber", "light": "lightNumber" },
    "syntaxConstant": { "dark": "darkNumber", "light": "lightNumber" },
    "syntaxOperator": { "dark": "darkFgMuted", "light": "lightFgMuted" },
    "syntaxPunctuation": { "dark": "darkFgMuted", "light": "lightFgMuted" },
    "syntaxProperty": { "dark": "darkCyan", "light": "lightCyan" },
    "syntaxTag": { "dark": "darkRed", "light": "lightRed" },
    "syntaxAttribute": { "dark": "darkPurple", "light": "lightPurple" }
  }
}
